<body>
<noscript>
    <div style="padding: 25px; background-color: yellow; color: black;" class="has-text-centered">
        <span class="icon">
            <i class="fas fa-exclamation-triangle"></i>
        </span>
        <span><%= __("common.jsDisabled") %></span>
    </div>
    <style>
        body {
            border-left-color: yellow;
            border-left-width: 10px;
            border-left-style: solid;
            border-right-color: yellow;
            border-right-width: 10px;
            border-right-style: solid;
            border-bottom-color: yellow;
            border-bottom-width: 10px;
            border-bottom-style: solid;
        }
    </style>
</noscript>
<% if (announcement.active === true) { %>
    <div id="announcementBody" <% if (announcement.colour === "preferred") { %>class="has-background-default has-text-centered" style="padding: 5px;"<% } else { %>style="background-color: <%= announcement.colour %>; color: <%= announcement.foreground %>; padding: 5px;" class="has-text-centered"<% } %>>
        <%- announcement.message %>
    </div>
<% } %>
<nav class="navbar" role="navigation" aria-label="main navigation">
    <div class="navbar-brand">
        <% if (title !== "Home") { %>
            <a class="navbar-item" href="/" aria-hidden="true">
                <img style="width: auto !important; height: 48px !important;" alt="<% __("common.nav.logoAlt") %>" src="/img/logo-128px.<%= imageFormat %>" draggable="false">
            </a>
        <% } %>

        <a role="button" class="navbar-burger burger" aria-label="menu" aria-expanded="false" data-target="navbar">
            <span aria-hidden="true"></span>
            <span aria-hidden="true"></span>
            <span aria-hidden="true"></span>
        </a>
    </div>

    <div id="navbar" class="navbar-menu">
        <div class="navbar-start"> 

            <a class="navbar-item" href="<%= linkPrefix %>/">
                <span class="icon iconalign">
                    <i class="fad fa-house fa-swap-opacity" aria-hidden="true"></i>
                </span>
                <span><%= __("common.home") %></span>
            </a>

            <a class="navbar-item" href="<%= linkPrefix %>/bots">
                <span class="icon iconalign">
                    <i class="fad fa-robot" aria-hidden="true"></i>
                </span>
                <span><%= __("common.bots") %></span>
            </a>

            <a class="navbar-item" href="<%= linkPrefix %>/servers">
                <span class="icon iconalign">
                    <i class="fad fa-comments-alt fa-swap-opacity" aria-hidden="true"></i>
                </span>
                <span><%= __("common.servers") %></span>
            </a>

            <a class="navbar-item" href="<%= linkPrefix %>/templates">
                <span class="icon iconalign">
                    <i class="fad fa-clone" aria-hidden="true"></i>
                </span>
                <span><%= __("common.templates") %></span>
            </a>

            <a class="navbar-item" href="<%= linkPrefix %>/search">
                <span class="icon iconalign">
                    <i class="fas fa-search" aria-hidden="true"></i>
                </span>
                <span><%= __("common.search") %></span>
            </a>

            <div <% if (phone) { %>onclick="display(document.getElementById('moreDropdown'), document.getElementById('moreDropdownArrow'));"<% } %> class="navbar-item has-dropdown is-hoverable">
                <a class="navbar-link <% if (phone) { %>is-arrowless<% } %>" tabindex="0">
                    <span class="icon iconalign">
                        <i class="fad fa-sitemap" aria-hidden="true"></i>
                    </span>
                    <span><%= __("common.nav.more") %></span>
                    <% if (phone) { %>
                        <span>
                            <i id="moreDropdownArrow" class="fas fa-chevron-left dropdown-arrow" aria-hidden="true"></i>
                        </span>
                    <% } %>
                </a>

                <div id="moreDropdown" class="navbar-dropdown is-hidden-mobile">
                    <a class="navbar-item" href="<%= linkPrefix %>/widgetbot" tabindex="0">
                        <span class="icon iconalign">
                            <i class="fab fa-discord" aria-hidden="true"></i>
                        </span>
                        <span><%= __("common.discord") %></span>
                    </a>
                    <a class="navbar-item" id="navAbout" href="<%= linkPrefix %>/about" tabindex="0">
                        <span class="icon iconalign">
                            <i class="fad fa-info-circle" aria-hidden="true"></i>
                        </span>
                        <span><%= __("common.nav.more.about") %></span>
                    </a>
                    <a class="navbar-item" href="https://discordextremelist.xyz/blog" target="_blank" rel="noopener" tabindex="0">
                        <span class="icon iconalign">
                            <i class="fad fa-pen" aria-hidden="true"></i>
                        </span>
                        <span><%= __("common.nav.more.blog") %></span>
                    </a>
                    <a class="navbar-item" href="https://status.discordextremelist.xyz" target="_blank" rel="noopener" tabindex="0">
                        <span class="icon iconalign">
                            <i class="fad fa-thermometer-three-quarters" aria-hidden="true"></i>
                        </span>
                        <span><%= __("common.nav.more.status") %></span>
                    </a>
                    <a class="navbar-item" href="<%= linkPrefix %>/docs" tabindex="0">
                        <span class="icon iconalign">
                            <i class="fad fa-books" aria-hidden="true"></i>
                        </span>
                        <span><%= __("common.nav.more.docs") %></span>
                    </a>
                    <hr class="navbar-divider">
                    <a class="navbar-item" href="<%= linkPrefix %>/terms" tabindex="0">
                        <span class="icon iconalign">
                            <i class="fad fa-balance-scale" aria-hidden="true"></i>
                        </span>
                        <span><%= __("common.nav.more.terms") %></span>
                    </a>
                    <a class="navbar-item" href="<%= linkPrefix %>/privacy" tabindex="0">
                        <span class="icon iconalign">
                            <i class="fad fa-user-secret" aria-hidden="true"></i>
                        </span>
                        <span><%= __("common.nav.more.privacy") %></span>
                    </a>
                    <a class="navbar-item" href="<%= linkPrefix %>/cookies" tabindex="0">
                        <span class="icon iconalign">
                            <i class="fad fa-cookie" aria-hidden="true"></i>
                        </span>
                        <span><%= __("common.nav.more.cookies") %></span>
                    </a>
                    <a class="navbar-item" href="<%= linkPrefix %>/guidelines" tabindex="0">
                        <span class="icon iconalign">
                            <i class="fad fa-book" aria-hidden="true"></i>
                        </span>
                        <span><%= __("common.nav.more.guidelines") %></span>
                    </a>
                </div>
            </div>
        </div>

        <div class="navbar-end">
            <div <% if (phone) { %>onclick="display(document.getElementById('localeDropdown'), document.getElementById('localeDropdownArrow'));"<% } %> class="navbar-item has-dropdown is-hoverable">
                <a class="navbar-link <% if (phone) { %>is-arrowless<% } %>" tabindex="0">
                    <span class="icon iconalign">
                        <i class="fad fa-language" aria-hidden="true"></i>
                    </span>
                    <span><%= __("common.nav.language") %></span>
                    <% if (phone) { %>
                        <span>
                            <i id="localeDropdownArrow" class="fas fa-chevron-left dropdown-arrow" aria-hidden="true"></i>
                        </span>
                    <% } %>
                </a>

                <div id="localeDropdown" class="navbar-dropdown is-hidden-mobile">
                    <a class="navbar-item" <% if (req.locale !== "en-NZ") { %>href="<%= req.originalUrl.replace(req.locale, "en-NZ") %>?setLang=t"<% } %> rel="nofollow" tabindex="0">
                        <% if (req.locale === "en-NZ") { %>
                            <span title="Selected" class="icon has-text-success iconalign">
                                <i class="fad fa-check-circle" aria-hidden="true"></i>
                            </span>
                        <% } %>
                        <span><%= __({ phrase: "common.nav.language.en-NZ", locale: "en-NZ" }) %></span>
                    </a>
                    <a class="navbar-item" <% if (req.locale !== "en-GB") { %>href="<%= req.originalUrl.replace(req.locale, "en-GB") %>?setLang=t"<% } %> rel="nofollow" tabindex="0">
                        <% if (req.locale === "en-GB") { %>
                            <span title="Selected" class="icon has-text-success iconalign">
                                <i class="fad fa-check-circle" aria-hidden="true"></i>
                            </span>
                        <% } %>
                        <span><%= __({ phrase: "common.nav.language.en-UK", locale: "en-GB" }) %></span>
                    </a>
                    <a class="navbar-item" <% if (req.locale !== "en-US") { %>href="<%= req.originalUrl.replace(req.locale, "en-US") %>?setLang=t"<% } %> rel="nofollow" tabindex="0">
                        <% if (req.locale === "en-US") { %>
                            <span title="Selected" class="icon has-text-success iconalign">
                                <i class="fad fa-check-circle" aria-hidden="true"></i>
                            </span>
                        <% } %>
                        <span><%= __({ phrase: "common.nav.language.en-US", locale: "en-US" }) %></span>
                    </a>
                    <hr class="navbar-divider">
                    <a class="navbar-item" <% if (req.locale !== "de-DE") { %>href="<%= req.originalUrl.replace(req.locale, "de-DE") %>?setLang=t"<% } %> rel="nofollow" tabindex="0">
                        <% if (req.locale === "de-DE") { %>
                            <span title="Selected" class="icon has-text-success iconalign">
                                <i class="fad fa-check-circle" aria-hidden="true"></i>
                            </span>
                        <% } %>
                        <span><%= __({ phrase: "common.nav.language.de", locale: "de-DE" }) %></span>
                    </a>
                    <a class="navbar-item" <% if (req.locale !== "es-ES") { %>href="<%= req.originalUrl.replace(req.locale, "es-ES") %>?setLang=t"<% } %> rel="nofollow" tabindex="0">
                        <% if (req.locale === "es-ES") { %>
                            <span title="Selected" class="icon has-text-success iconalign">
                                <i class="fad fa-check-circle" aria-hidden="true"></i>
                            </span>
                        <% } %>
                        <span><%= __({ phrase: "common.nav.language.es-ES", locale: "es-ES" }) %></span>
                    </a>
                    <a class="navbar-item" <% if (req.locale !== "he-IL") { %>href="<%= req.originalUrl.replace(req.locale, "he-IL") %>?setLang=t"<% } %> rel="nofollow" tabindex="0">
                        <% if (req.locale === "he-IL") { %>
                            <span title="Selected" class="icon has-text-success iconalign">
                                <i class="fad fa-check-circle" aria-hidden="true"></i>
                            </span>
                        <% } %>
                        <span><%= __({ phrase: "common.nav.language.he", locale: "he-IL" }) %></span>
                    </a>
                    <a class="navbar-item" <% if (req.locale !== "hu-HU") { %>href="<%= req.originalUrl.replace(req.locale, "hu-HU") %>?setLang=t"<% } %> rel="nofollow" tabindex="0">
                        <% if (req.locale === "hu-HU") { %>
                            <span title="Selected" href="<%= req.originalUrl.replace(req.locale, "hu-HU") %>" class="icon has-text-success iconalign">
                                <i class="fad fa-check-circle" aria-hidden="true"></i>
                            </span>
                        <% } %>
                        <span><%= __({ phrase: "common.nav.language.hu", locale: "hu-HU" }) %></span>
                    </a>
                    <a class="navbar-item" <% if (req.locale !== "tr-TR") { %>href="<%= req.originalUrl.replace(req.locale, "tr-TR") %>?setLang=t"<% } %> rel="nofollow" tabindex="0">
                        <% if (req.locale === "tr-TR") { %>
                            <span title="Selected" class="icon has-text-success iconalign">
                                <i class="fad fa-check-circle" aria-hidden="true"></i>
                            </span>
                        <% } %>
                        <span><%= __({ phrase: "common.nav.language.tr-TR", locale: "tr-TR" }) %></span>
                    </a>
                    <hr class="navbar-divider">
                    <a class="navbar-item" target="_blank" rel="noopener" href="https://translate.discordextremelist.xyz/" tabindex="0">
                        <span>
                            <%= __("common.nav.language.helpUs") %>
                        </span>
                    </a>
                    <% if (req.session.disableRTL === true) { %>
                        <a class="navbar-item" href="<%= req.originalUrl %>?localeLayout=rtl" tabindex="0">
                            <span title="Selected" class="icon has-text-success iconalign">
                                <i class="fad fa-check-circle" aria-hidden="true"></i>
                            </span>
                            <span class="has-text-success">
                                <%= __("common.nav.language.enableRTL") %>
                            </span>
                        </a>
                    <% } else if (!req.session.disableRTL && req.session.rtlLanguage) { %>
                        <a class="navbar-item" href="<%= req.originalUrl %>?localeLayout=ltr" tabindex="0">
                            <span title="Selected" class="icon has-text-danger iconalign">
                                <i class="fad fa-times-circle" aria-hidden="true"></i>
                            </span>
                            <span class="has-text-danger">
                                <%= __("common.nav.language.disableRTL") %>
                            </span>
                        </a>
                    <% } %>
                </div>
            </div>

            <% if (req.user) { %>
                <div <% if (phone) { %>onclick="display(document.getElementById('userDropdown'), document.getElementById('userDropdownArrow'));"<% } %> class="navbar-item has-dropdown is-hoverable">
                    <a class="navbar-link <% if (phone) { %>is-arrowless<% } %>" tabindex="0">
                        <img style="margin-right: 6px; border-radius: 50%; vertical-align: middle;" src="<%= req.user.db.avatar.url %>.<%= imageFormat %>?size=256" alt="<%= req.user.db.fullUsername %>'s Discord Profile Picture" onerror=<%= "this.onerror=null,this.src=&quot;/img/defaultavatar." + imageFormat + "&quot;" %> type="image/<%= imageFormat %>" draggable="false">
                        <span style="vertical-align: middle;">&nbsp; <%= req.user.db.fullUsername %> </span>
                        <% if (phone) { %>
                            <span>
                                <i id="userDropdownArrow" class="fas fa-chevron-left dropdown-arrow" aria-hidden="true"></i>
                            </span>
                        <% } %>
                    </a>

                    <div id="userDropdown" class="navbar-dropdown is-hidden-mobile is-right">
                        <a class="navbar-item" href="<%= linkPrefix %>/users/@me" tabindex="0">
                            <span class="icon iconalign">
                                <i class="fad fa-user fa-swap-opacity" aria-hidden="true"></i>
                            </span>
                            <span><%= __("common.nav.me.profile") %></span>
                        </a>
                        <a class="navbar-item" href="<%= linkPrefix %>/users/account/preferences" tabindex="0">
                            <span class="icon iconalign">
                                <i class="fad fa-swatchbook" aria-hidden="true"></i>
                            </span>
                            <span><%= __("common.nav.me.preferences") %></span>
                        </a>
                        <% if (req.user.db.rank.mod === true) { %>
                            <a class="navbar-item" href="<%= linkPrefix %>/staff" tabindex="0">
                                <span class="icon iconalign">
                                    <i class="fad fa-cogs" aria-hidden="true"></i>
                                </span>
                                <span><%= __("common.nav.me.staffPanel") %></span>
                            </a>
                        <% } %>
                        <a class="navbar-item" href="<%= linkPrefix %>/bots/submit" tabindex="0">
                            <span class="icon iconalign">
                                <i class="fad fa-robot" aria-hidden="true"></i>
                            </span>
                            <span><%= __("common.nav.me.submitBot") %></span>
                        </a>
                        <a class="navbar-item" href="<%= linkPrefix %>/servers/submit" tabindex="0">
                            <span class="icon iconalign">
                                <i class="fad fa-comments-alt fa-swap-opacity" aria-hidden="true"></i>
                            </span>
                            <span><%= __("common.nav.me.submitServer") %></span>
                        </a>
                        <a class="navbar-item" id="navTemplate" href="<%= linkPrefix %>/templates/submit" tabindex="0">
                            <span class="icon iconalign">
                                <i class="fad fa-clone" aria-hidden="true"></i>
                            </span>
                            <span><%= __("common.nav.me.submitTemplate") %></span>
                        </a>
                        <% if (req.user.db.preferences.enableGames === true) { %>
                            <hr class="navbar-divider">
                            <a class="navbar-item" href="<%= linkPrefix %>/users/game/snake" tabindex="0">
                                <span class="icon iconalign">
                                    <i class="fad fa-snake" aria-hidden="true"></i>
                                </span>
                                <span><%= __("common.nav.me.playSnake") %></span>
                            </a>
                            <a class="navbar-item" id="navSnake" href="<%= linkPrefix %>/users/game/snake/leaderboard" tabindex="0">
                                <span class="icon iconalign">
                                    <i class="fad fa-podium-star" aria-hidden="true"></i>
                                </span>
                                <span><%= __("common.nav.me.snakeLB") %></span>
                            </a>
                        <% } %>
                        <% if (mobile) { %>
                            <% if (ddosMode === true) { %>
                                <a class="navbar-item has-text-danger" title="<%= __("common.notice.ddosMode") %>" disabled tabindex="0">
                                    <span class="icon iconalign">
                                        <i class="fad fa-sign-out" aria-hidden="true"></i>
                                    </span>
                                    <span><%= __("common.logout") %></span>
                                </a>
                            <% } else { %>
                                <a class="navbar-item has-text-danger" href="/auth/logout" tabindex="0">
                                    <span class="icon iconalign">
                                        <i class="fad fa-sign-out" aria-hidden="true"></i>
                                    </span>
                                    <span><%= __("common.logout") %></span>
                                </a>
                            <% } %>
                        <% } %>
                    </div>
                </div>
            <% } %>
            <div class="navbar-item" id="authButtons">
                <% if (!mobile) { %>
                    <div class="buttons">
                        <% if (req.user) { %>
                            <% if (ddosMode === true) { %>
                                <a class="button is-danger" title="<%= __("common.notice.ddosMode") %>" disabled>
                                    <span class="icon">
                                        <i class="fad fa-sign-out" aria-hidden="true"></i>
                                    </span>
                                    <span><%= __("common.logout") %></span>
                                </a>
                            <% } else { %>
                                <a class="button is-danger" href="/auth/logout">
                                    <span class="icon">
                                        <i class="fad fa-sign-out" aria-hidden="true"></i>
                                    </span>
                                    <span><%= __("common.logout") %></span>
                                </a>
                            <% } %>
                        <% } else { %>
                            <% if (ddosMode === true) { %>
                                <a class="button is-blurple" <% if (hideLogin && hideLogin === true) { %>style="display: none;"<% } %> title="<%= __("common.notice.ddosMode") %>" disabled>
                                    <span class="icon">
                                        <i class="fab fa-discord" aria-hidden="true"></i>
                                    </span>
                                    <span><%= __("common.login") %></span>
                                </a>
                            <% } else { %>
                                <a class="button is-blurple" <% if (hideLogin && hideLogin === true) { %>style="display: none;"<% } %> href="/auth/login">
                                    <span class="icon">
                                        <i class="fab fa-discord" aria-hidden="true"></i>
                                    </span>
                                    <span><%= __("common.login") %></span>
                                </a>
                            <% } %>
                        <% } %>
                    </div>
                <% } else if (!req.user) { %>
                    <% if (ddosMode === true) { %>
                        <a class="has-text-blurple" <% if (hideLogin && hideLogin === true) { %>style="display: none;"<% } %> title="<%= __("common.notice.ddosMode") %>" disabled>
                            <span class="icon iconalign">
                                <i class="fab fa-discord" aria-hidden="true"></i>
                            </span>
                            <span><%= __("common.login") %></span>
                        </a>
                    <% } else { %>
                        <a class="has-text-blurple" <% if (hideLogin && hideLogin === true) { %>style="display: none;"<% } %> href="/auth/login">
                            <span class="icon iconalign">
                                <i class="fab fa-discord" aria-hidden="true"></i>
                            </span>
                            <span><%= __("common.login") %></span>
                        </a>
                    <% } %>
                <% } %>
            </div>
        </div>
    </div>
</nav>
<div id="siteContent">
